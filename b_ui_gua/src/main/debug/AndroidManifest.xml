            </intent-filter>

        </activity>


        <meta-data
            android:name="com.wd.common.life.GlobalConfiguration"
            android:value="ConfigModule" />

        <meta-data
            android:name="com.wd.gua.life.GlobalConfiguration"
            android:value="ConfigModule" />
